{
  "name": "n8n-nodes-ondo-finance",
  "version": "1.0.0",
  "description": "n8n community node for Ondo Finance API integration",
  "keywords": [
    "n8n-community-node-package",
    "ondo-finance",
    "defi",
    "tokenization",
    "real-world-assets"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/velocity-bpa/n8n-nodes-ondo-finance",
  "author": {
    "name": "Velocity BPA",
    "email": "engineering@velocitybpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velocity-bpa/n8n-nodes-ondo-finance.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.0",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OndoFinanceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OndoFinance/OndoFinance.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "*"
  }
}